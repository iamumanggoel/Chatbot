{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.py)",
      "dest": "index.py"
    },
    {
      "src": "/",
      "dest": "index.py"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "index.py"
    }
  ]
}
